[mysqld]
# MariaDB base config - networking controlled by runtime.cnf
binlog_format=ROW
default-storage-engine=innodb
innodb_autoinc_lock_mode=2
socket=/run/mysqld/mysqld.sock
# log_error is set to console by mariadbd-safe when not specified
# Note: bind-address, port, skip-networking are set in /etc/my.cnf.d/runtime.cnf
# wsrep settings are also set in runtime.cnf based on ENABLE_GALERA env var

[mariadbd-safe]
skip_log_error
